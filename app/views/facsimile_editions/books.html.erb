<!--[if !IE]>menulocation<![endif]-->
<div id="menulocation">
  <%# set up the subtypes to be shown in the tabs  %>
  <%= widget(:simple_mode_tabs, :tabs_elements => book_tabs) %>
</div>
<!--[if !IE]>menulocation<![endif]-->

<div id="description">
</div>
<!--[if !IE]>description<![endif]-->
<div id="content">
  <ul id="access_list" class="minilist facsimile_books">
    <% @books.each do |book| %>
      <li>
        <h1>
          <%= thumb_link(book, :border => '0', :height => '81', :width => '54' ) %>
           <%= book.siglum %>
        </h1>
        <p>
          <%= material_description(book) %>
        </p>
      </li>
    <%end%>
  </ul>
</div>

