<!-- navigate -->
<div id="menulocation">
  <%= widget(:simple_mode_tabs, :tabs_elements => tabs_elements) %>
</div>
<!-- menulocation -->

<div id="description">
  <p></p>
</div>
<!--[if !IE]>description<![endif]-->
<div id="content">
  <ul id="access_list" class="minilist">
    <% @books.each do |book| %>
      <li>
        <h1>
          <%= thumb_link(book, :border => '0' ) %>
          <% url = book.uri.to_s %>        
          <% title = book.uri.local_name %>
          <% text = "<img src='#{url}.jpeg?size=thumbnail' alt='#{title}' border='0'/>#{title}" %>        
          <%= titled_link(url, text, title)%>
        </h1>
        <p>
          <%= material_description(book) %>
        </p>
      </li>
    <%end%>    
  </ul>
</div>

