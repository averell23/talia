<% for supertype in @widget.supertypes %>
<h3><%= type_link("<< #{supertype.to_name_s}", supertype) %></h3>
<% end %>
<% if(@widget.source_class) %>
<h3> <%= @widget.source_class.to_name_s %></h3>
<% end %>
<% for subtype in @widget.subtypes %>
<h3><%= type_link("#{subtype.to_name_s} >>", subtype) %></h3>
<% end %>
