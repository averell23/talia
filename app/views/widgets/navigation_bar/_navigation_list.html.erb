<%#
# This is the template to render just the "current" list of navigation items,
# using the current navigation level
%>

<ul id="ipod_nav_level_<%= current_level %>">
  <%= type_link_up("Back") if(show_home) %>
  <% for supertype in widget_supertypes %>
    <li class="ipod_navigation_back_link" id="<%="#{ supertype.to_name_s('_') }"%>">
        <%= type_link_up(supertype) %>
    </li>
  <% end %>
  <% for subtype in widget_subtypes %>
    <li class="ipod_navigation_down_link" id="<%="#{ subtype.to_name_s('_') }"%>"> 
      <%= type_link_down("#{widget.class_label(subtype)}", subtype, current_level) %> 
    </li>
  <% end %>
</ul>
