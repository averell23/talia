  <owl:ObjectProperty rdf:ID="path_source">
    <rdfs:domain rdf:resource="#PathStep"/>
    <rdfs:range rdf:resource="#Source"/>
    <rdfs:comment xml:lang="en">The Source related to this path step</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="language">
    <rdfs:domain rdf:resource="#Contribution"/>
    <rdfs:comment xml:lang="en">Language of the contribuation</rdfs:comment>
    <owl:versionInfo xml:lang="en">TODO: Should use controlled vocabulary</owl:versionInfo>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="refers">
    <rdfs:range rdf:resource="#Source"/>
    <rdfs:domain rdf:resource="#Source"/>
    <rdfs:comment xml:lang="en">A reference to another source</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="note">
    <rdfs:domain rdf:resource="#Paragraph"/>
    <rdfs:range rdf:resource="#Note"/>
    <rdfs:comment xml:lang="en">A note for this paragraph</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="author">
    <rdfs:comment xml:lang="en">The author of a contribution</rdfs:comment>
    <rdfs:range rdf:resource="#Author"/>
    <rdfs:domain rdf:resource="#Contribution"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="path">
    <rdfs:domain rdf:resource="#PathStep"/>
    <rdfs:range rdf:resource="#Path"/>
    <rdfs:comment xml:lang="en">The path to which the step belongs</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="content_type">
    <rdfs:range rdf:resource="#ContributionContentType"/>
    <rdfs:comment xml:lang="en">Describes the content of the contribution</rdfs:comment>
    <rdfs:domain rdf:resource="#Contribution"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="author_position">
    <rdfs:comment xml:lang="en">Position of the author</rdfs:comment>
    <rdfs:domain rdf:resource="#Author"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="cites">
    <rdfs:subPropertyOf rdf:resource="#refers"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="material">
    <rdfs:domain rdf:resource="#Contribution"/>
    <rdfs:range rdf:resource="#Material"/>
    <rdfs:comment xml:lang="en">Reference to a related material</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#refers"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="translation_of">
    <rdfs:subPropertyOf rdf:resource="#refers"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="first_page">
    <rdfs:comment xml:lang="en">The first page of a chapter or other element</rdfs:comment>
    <rdfs:domain rdf:resource="#Chapter"/>
    <rdfs:range rdf:resource="#Page"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="material_type">
    <rdfs:range rdf:resource="#StructureElement"/>
    <rdfs:domain rdf:resource="#Material"/>
    <rdfs:comment xml:lang="en">Indicates the material type of the Material</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="is_in_archive">
    <rdfs:comment xml:lang="en">the book is in this archive</rdfs:comment>
    <rdfs:range rdf:resource="#Archive"/>
    <rdfs:domain rdf:resource="#Book"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="part_of">
    <rdfs:domain rdf:resource="#Source"/>
    <rdfs:range rdf:resource="#Source"/>
    <rdfs:comment xml:lang="en">Indicates that one element is part of another</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="related_contribution">
    <rdfs:comment xml:lang="en">Indicates that a source is closely related to a contribution</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#refers"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="page">
    <rdfs:range rdf:resource="#Page"/>
    <rdfs:comment xml:lang="en">Page to which the Zone belongs</rdfs:comment>
    <rdfs:domain rdf:resource="#Note"/>
  </owl:ObjectProperty>
  <rdf:Property rdf:ID="series">
    <rdfs:range rdf:resource="#Series"/>
    <rdfs:domain rdf:resource="#Source"/>
    <rdfs:comment xml:lang="en">Relates a Source with the series in which it was published</rdfs:comment>
  </rdf:Property>
  <rdf:Property rdf:ID="play_length">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#time"/>
    <rdfs:domain rdf:resource="#AvMedia"/>
    <rdfs:comment xml:lang="en">The play length of this media file.</rdfs:comment>
  </rdf:Property>
  <rdf:Property rdf:ID="downloadable">
    <rdfs:comment xml:lang="en">Indicates if the media file may be downloaded</rdfs:comment>
    <rdfs:domain rdf:resource="#AvMedia"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </rdf:Property>
  <rdf:Property rdf:ID="category">
    <rdfs:comment xml:lang="en">Assigns a category to a source.</rdfs:comment>
    <rdfs:domain rdf:resource="#Source"/>
    <rdfs:range rdf:resource="#Category"/>
  </rdf:Property>
  <owl:DatatypeProperty rdf:ID="position">
    <rdfs:comment xml:lang="en">Position in a collection of elements</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#Source"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="filename">
    <rdfs:comment xml:lang="en">File containing the contribution</rdfs:comment>
    <rdfs:domain rdf:resource="#Contribution"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="already_published">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="#Contribution"/>
    <rdfs:comment xml:lang="en">Indicates if the contribution had already been published before being published in the Hyper</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="zip">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment xml:lang="en">Zip code</rdfs:comment>
    <rdfs:domain rdf:resource="#Agent"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="email">
